{
  "name": "Hide'em",
  "description": "Hide sections with changes on bitbucket review page.",
  "version": "2.0",

  "manifest_version": 2,

  "content_scripts": [
    {
      "matches": [
        "https://bitbucket.org/edgelab/*"
      ],
      "js": [
        "bower_components/jquery/dist/jquery.js",
        "script.js"
      ],
      "run_at": "document_end"
    }
    ]
}